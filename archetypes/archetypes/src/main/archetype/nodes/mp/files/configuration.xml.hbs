<component name="ProjectRunConfigurationManager">
    <configuration default="false" name="{{artifactId}}" type="Application" factoryName="Application">
        {{#mainClass}}
            <option name="MAIN_CLASS_NAME" value="{{mainClass}}" />
        {{/mainClass}}
        {{^mainClass}}
            <option name="MAIN_CLASS_NAME" value="io.helidon.Main" />
        {{/mainClass}}
        <module name="{{artifactId}}" />
        <method v="2">
            <option name="Make" enabled="true" />
        </method>
    </configuration>
</component>